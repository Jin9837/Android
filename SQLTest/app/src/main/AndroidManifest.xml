<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sqltest">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SQLTest">
        <activity
            android:name=".UpdateActivity"
            android:exported="true"
            android:label="Update Data"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".QueryActivity"
            android:exported="true"
            android:label="Query Data"
            android:parentActivityName=".MainActivity"/> <!--  可以通过左上角箭头返回父页面 -->
        <activity
            android:name=".DeleteActivity"
            android:exported="true"
            android:label="Delete Data"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".InsertActivity"
            android:exported="true"
            android:label="Insert Data"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>